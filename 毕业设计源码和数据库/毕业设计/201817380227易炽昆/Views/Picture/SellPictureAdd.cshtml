
@{
    ViewBag.Title = "SellPictureAdd";
    Layout = "~/Views/BackstageView.cshtml";
}

@using _201817380227易炽昆.Models
@{
    SellHouse sellHouse = ViewBag.sellHouse;
}

@* 面包屑导航 *@
<nav aria-label="breadcrumb">
    <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="/BackStage/Index">首页</a></li>
        <li class="breadcrumb-item"><a href="#">出售房信息</a></li>
        <li class="breadcrumb-item"><a href="/Picture/SellPicture">房屋图片管理</a></li>
        <li class="breadcrumb-item active" aria-current="page">添加图片</li>
    </ol>
</nav>
<div style="width:80%;margin-left:10%;margin-top:6%;">
    <form action="/Picture/SellPictureAdd" method="post" enctype="multipart/form-data">
        <div class="form-group row">
            <label for="file" class="col-sm-2 col-form-label">请选择图片：</label>
            <div class="col-sm-10">
                <input type="file" class="form-control" multiple id="file" name="file">
            </div>
        </div>
        <div class="form-group row">
            <div class="col-sm-10">
                <input type="hidden" name="SellID" value="@sellHouse.SellID" />
                <input type="submit" class="btn btn-primary">
            </div>
        </div>
    </form>
</div>

