
@{
    ViewBag.Title = "PictureAdd";
    Layout = "~/Views/BackstageView.cshtml";
}

@using _201817380227易炽昆.Models
@{
    LeaseHouse lease = ViewBag.lease;
}

@* 面包屑导航 *@
<nav aria-label="breadcrumb">
    <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="/BackStage/Index">首页</a></li>
        <li class="breadcrumb-item"><a href="#">出租房信息</a></li>
        <li class="breadcrumb-item"><a href="/Picture/LeasePicture">房屋图片管理</a></li>
        <li class="breadcrumb-item active" aria-current="page">添加图片</li>
    </ol>
</nav>
<div style="width:80%;margin-left:10%;margin-top:6%;">
    <form action="/Picture/PictureAdd" method="post" enctype="multipart/form-data">
        <div class="form-group row">
            <label for="file" class="col-sm-2 col-form-label">请选择图片：</label>
            <div class="col-sm-10">
                <input type="file" class="form-control" multiple id="file" name="file">
            </div>
        </div>
        <div class="form-group row">
            <div class="col-sm-10">
                <input type="hidden" name="LeaseID" value="@lease.LeaseID" />
                <input type="submit" class="btn btn-info"  value="添加">
                <a href="javascript:history.go(-1);" class="btn btn-info" style="width:200px;">返回</a>
            </div>
        </div>
    </form>
</div>

